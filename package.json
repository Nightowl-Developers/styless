{
  "name": "styless",
  "version": "0.0.1",
  "description": "A component library that is semantically ada compliant but lacks any styles.",
  "main": "dist/scl/index.cjs.js",
  "module": "dist/scl/index.ejs.js",
  "types": "dist/scl/index.d.ts",
  "repository": "http://gitlab.joshuablevins.net/root/styless.git",
  "author": "Joshua Blevins <jblevins@nightowl-developers.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --config .eslintrc.json --ext .tsx --ext .ts",
    "lint:fix": "yarn lint --fix",
    "storybook:build": "build-storybook --output-dir dist/storybook",
    "storybook:start": "start-storybook -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:out": "jest --json --outputFile=.jest-test-results.json",
    "uxpin:dev": "",
    "uxpin:push": "",
    "containerize:storybook": "docker build -t styless/storybook - < Dockerfiles/storybook.Dockerfile",
    "containerize:coverage": "docker build -t styless/coverage - < Dockerfiles/coverage.Dockerfile"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-jest": "^6.4.19",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-loader": "^8.2.3",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.7",
    "react-hooks-testing-library": "^0.6.0",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.67.0",
    "storybook-addon-pseudo-states": "^1.0.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
